{
  "build": {
    "content": [
      {
        "files": [
          "**/toc.md",
          "**/toc.yml"
        ],
        "exclude": [
          "excel-scripts/**"
        ],
        "src": "docs-ref-autogen",
        "dest": "api/office_ref_toc"
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-conceptual",
        "dest": "office",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "**/_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-autogen",
        "dest": "api",
        "exclude": [
          "excel-scripts/**",
          "excel-scripts.yml",
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "**/_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },

      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-autogen/excel-scripts",
        "dest": "api"
      },
      {
        "files": [
          "excel.yml"
        ],
        "src": "docs-ref-autogen",
        "dest": "api"
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "breadcrumb",
        "dest": "office_scripts_breadcrumb"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "dest": "api"
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "src": "docs-ref-conceptual",
        "dest": "office"
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "uhfHeaderId": "MSDocsHeader-Dev_Office",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "extendBreadcrumb": true,
      "ms.suite": "office",
      "ms.author": "o365devx",
      "apiPlatform": "javascript",
      "author": "o365devx",
      "ms.technology": "scripts",
      "ms.topic": "reference",
      "titleSuffix": "Office Scripts",
      "ms.devlang": "JavaScript",
      "product": [
        "office 365"
      ],
      "feedback_system": "GitHub",
      "feedback_github_repo": "OfficeDev/office-scripts-docs-reference",
      "feedback_product_url": "https://excel.uservoice.com/forums/274580-excel-for-the-web?category_id=143439"
    },
    "fileMetadata": {
      "feedback_system": {
        "docs-ref-conceptual/**/*": "GitHub"
      },
      "feedback_github_repo": {
        "docs-ref-conceptual/**/*": "OfficeDev/office-scripts-docs-reference"
      },
      "feedback_product_url": {
        "docs-ref-conceptual/**/*": "https://excel.uservoice.com/forums/274580-excel-for-the-web?category_id=143439"
      },
      "product": {
        "docs-ref-autogen/excel-scripts/**": [
          "office scripts"
        ]
      },
      "ms.prod": {
        "docs-ref-autogen/excel-scripts/**": "excel"
      }
    },
    "template": [],
    "dest": "office-scripts-docs-reference"
  }
}
