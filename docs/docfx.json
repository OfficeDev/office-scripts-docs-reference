{
  "build": {
    "content": [
      {
        "files": [
          "**/toc.md",
          "**/toc.yml"
        ],
        "exclude": [
          "excel/**"
        ],
        "src": "docs-ref-autogen",
        "dest": "api/office-scripts/office_ref_toc"
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-conceptual",
        "dest": "office-scripts",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "**/_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-autogen",
        "dest": "api/office-scripts",
        "exclude": [
          "excel/**",
          "excel.yml",
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "**/_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "office-scripts",
        "src": "docs-ref-autogen/excel",
        "dest": "api/office-scripts"
      },
      {
        "files": [
          "excel.yml"
        ],
        "group": "office-scripts",
        "src": "docs-ref-autogen/excel",
        "dest": "api/office-scripts"
      },
      {
        "files": [
          "toc.yml"
        ],
        "group": "office-scripts",
        "src": "breadcrumb",
        "dest": "office_scripts_breadcrumb"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
		      "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "dest": "api"
      }
    ],
    "groups": {
      "office-scripts": {
        "dest": "office-scripts",
        "moniker_range": "office-scripts"
      }
    },
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "uhfHeaderId": "office-scripts",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "extendBreadcrumb": true,
      "ms.suite": "office",
      "ms.author": "o365devx",
      "apiPlatform": "javascript",
      "author": "o365devx",
      "ms.subservice": "scripts",
      "ms.service": "excel",
      "ms.topic": "reference",
      "titleSuffix": "Office Scripts",
      "ms.devlang": "JavaScript",
      "product": [
        "office 365"
      ],
      "products": [
        "https://authoring-docs-microsoft.poolparty.biz/devrel/60932d05-feee-4685-a73b-595e25dd9318",
        "https://authoring-docs-microsoft.poolparty.biz/devrel/6ab06385-661e-4214-8870-bbe4071c960d"
      ],
      "feedback_system": "GitHub",
      "feedback_github_repo": "OfficeDev/office-scripts-docs-reference",
      "feedback_product_url": "/answers/tags/321/office-development"
    },
    "fileMetadata": {
      "feedback_system": {
        "docs-ref-conceptual/**/*": "GitHub"
      },
      "feedback_github_repo": {
        "docs-ref-conceptual/**/*": "OfficeDev/office-scripts-docs-reference"
      },
      "feedback_product_url": {
        "docs-ref-conceptual/**/*": "/answers/tags/321/office-development"
      },
      "product": {
        "docs-ref-autogen/excel/**": [
          "office scripts"
        ]
      },
      "ms.topic": {
        "docs-ref-autogen/**": "language-reference"
      }
    },
    "template": [],
    "dest": "office-scripts-docs-reference"
  }
}
