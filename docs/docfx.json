{
  "build": {
    "content": [
      {
        "files": [
          "**/toc.md",
          "**/toc.yml"
        ],
        "exclude": [
          "excel/**"
        ],
        "src": "docs-ref-autogen",
        "dest": "api/office-scripts/office_ref_toc"
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "src": "docs-ref-conceptual",
        "dest": "office-scripts",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "**/_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "office-scripts",
        "src": "docs-ref-autogen",
        "dest": "api/office-scripts",
        "exclude": [
          "excel/**",
          "excel.yml",
          "**/obj/**",
          "**/includes/**",
          "**/_themes/**",
          "**/_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "toc.yml"
        ],
        "group": "office-scripts",
        "src": "breadcrumb",
        "dest": "office_scripts_breadcrumb"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
		      "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ],
        "dest": "api"
      }
    ],
    "groups": {
      "office-scripts": {
        "dest": "office-scripts",
        "moniker_range": "office-scripts"
      }
    },
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "uhfHeaderId": "office-scripts",
      "breadcrumb_path": "~/breadcrumb/toc.yml",
      "extendBreadcrumb": true,
      "ms.suite": "office",
      "ms.author": "o365devx",
      "apiPlatform": "javascript",
      "author": "o365devx",
      "ms.subservice": "scripts",
      "ms.service": "excel",
      "ms.topic": "reference",
      "feedback_system": "OpenSource",
      "feedback_product_url": "/answers/tags/321/office-development",
      "open_source_feedback_contributorGuideUrl": "https://github.com/OfficeDev/office-scripts-docs-reference/blob/main/Contributing.md",
      "open_source_feedback_issueTitle": "",
      "open_source_feedback_issueUrl": "https://github.com/OfficeDev/office-scripts-docs-reference/issues/new?template=3_customer-feedback.yml",
      "open_source_feedback_productName": "Office Scripts",
      "titleSuffix": "Office Scripts",
      "ms.devlang": "typescript",
      "products": [
        "https://authoring-docs-microsoft.poolparty.biz/devrel/c43bb58b-1190-419b-8d18-e6052371b599"
      ]
    },
    "fileMetadata": {
      "ms.topic": {
        "docs-ref-autogen/**": "generated-reference"
      }
    },
    "template": [],
    "dest": "office-scripts-docs-reference"
  }
}
